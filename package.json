		"colors": "^1.4.0",
		"cssnano": "^7.0.6",
		"del": "^8.0.0",
		"eslint": "^9.17.0",
		"eslint-config-prettier": "^10.0.1",
		"gulp": "^5.0.0",
		"gulp-cheerio": "^1.0.0",
		"gulp-data": "^1.3.1",
		"gulp-notify": "^5.0.0",
